<launch>
  <master auto="start"/>

  <!-- Run the map server -->
  <arg name="map_file" default="/home/jessyxie/dumpling_ws/src/navigation-pkg/map_server/test/testmap.yaml"/>

  <!-- Run the map server -->
  <node name="map_server" pkg="map_server" type="map_server" args="$(arg map_file)" />


  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
    <rosparam file="$(find dumpling_2dnav)/params/costmap_common_params.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find dumpling_2dnav)/params/costmap_common_params.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find dumpling_2dnav)/params/local_costmap_params.yaml" command="load" />
    <rosparam file="$(find dumpling_2dnav)/params/global_costmap_params.yaml" command="load" />
    <rosparam file="$(find dumpling_2dnav)/params/base_local_planner_params.yaml" command="load" />
  </node>
</launch>
